    "psr/http-server-middleware": "^1.0",
    "psr/log": "^3.0",
    "ramsey/uuid": "^4.7",
    "slim/slim": "^4.13",
    "slim/twig-view": "^3.3",
    "spiral/roadrunner": "^2023.3",
    "spiral/roadrunner-http": "^3.4",
    "spiral/roadrunner-metrics": "^3.1",
    "spiral/roadrunner-worker": "^3.4",
    "symfony/cache": "^7.0",
    "symfony/stopwatch": "^7.0",
    "twig/twig": "^3.8"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "maglnet/composer-require-checker": "^4.8"
  },
  "autoload": {
    "psr-4": {
      "ToniLiesche\\Roadrunner\\": "src/"
    }
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": false
    }
  }
